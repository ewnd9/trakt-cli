{
  "name": "trakt-cli",
  "version": "0.2.1",
  "description": "access trakt from CLI",
  "main": "dist/index.js",
  "scripts": {
    "start": "node cli.js",
    "test": "ava",
    "test:watch": "nodemon --exec ava",
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "babel -d dist/ src/",
    "build:watch": "babel --watch -d dist/ src/",
    "prepublish": "npm run build",
    "style": "eslint src/*"
  },
  "preferGlobal": "true",
  "bin": {
    "trakt": "./cli.js"
  },
  "engines": {
    "node": ">=4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ewnd9/trakt-cli.git"
  },
  "keywords": [
    "trakt",
    "shows",
    "movies"
  ],
  "author": "ewnd9 <ewndnine@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.1",
    "inquirer-bluebird": "^1.0.6",
    "inquirer-credentials": "^2.0.0",
    "meow": "^3.7.0",
    "show-episode-format": "^1.0.1",
    "trakt-utils": "^0.3.0",
    "update-notifier": "^0.6.0"
  },
  "devDependencies": {
    "ava": "^0.9.0",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "nodemon": "^1.8.1",
    "eslint": "^1.10.3",
    "babel-eslint": "^5.0.0-beta9"
  }
}
